    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "patch-package": "^8.0.0",
    "react-devtools": "^4.28.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
